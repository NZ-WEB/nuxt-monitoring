{
  "name": "nuxt-monitoring",
  "version": "0.10.0",
  "description": "A Nuxt module for observability: debug server, health checks, and Prometheus metrics collection",
  "repository": "https://github.com/NZ-WEB/nuxt-monitoring",
  "license": "MIT",
  "author": "NZ-WEB",
  "keywords": [
    "nuxt",
    "nuxt-module",
    "monitoring",
    "metrics",
    "prometheus",
    "observability",
    "health-check",
    "debug-server"
  ],
  "homepage": "https://github.com/NZ-WEB/nuxt-monitoring#readme",
  "bugs": "https://github.com/NZ-WEB/nuxt-monitoring/issues",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "npm run dev:prepare && nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "release:patch": "npm run test && npm run lint && npm run prepack && changelogen --release --patch && npm publish && git push --follow-tags",
    "release:minor": "npm run test && npm run lint && npm run prepack && changelogen --release --minor && npm publish && git push --follow-tags",
    "release:major": "npm run test && npm run lint && npm run prepack && changelogen --release --major && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "check": "nuxi typecheck",
    "commitlint": "commitlint --edit $1",
    "prepare": "husky"
  },
  "dependencies": {
    "@nuxt/kit": "^4.2.2",
    "consola": "^3.4.2",
    "prom-client": "^15.1.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "@nuxt/devtools": "^3.1.1",
    "@nuxt/eslint-config": "^1.12.1",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.2.2",
    "@types/node": "latest",
    "changelogen": "^0.6.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.39.2",
    "husky": "^9.1.7",
    "nuxt": "^4.2.2",
    "typescript": "~5.9.3",
    "vitest": "^4.0.16",
    "vue-tsc": "^3.2.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
